---
import { Image } from "astro:assets";

const { navselect } = Astro.props;
import {
  BLOG_NAME,
  BLOG_DESCRIPTION,
  BLOG_AUTHOR,
  BLOG_THUMBNAIL,
} from "@/const";

const SITE_DOMAIN = Astro.site;
---

<header class="text-gray-600 body-font">
  <div class="container mx-auto flex p-5 flex-row">
    <a
      data-astro-prefetch
      href="/"
      class="flex title-font font-medium items-center text-gray-900 mb-4 md:mb-0 hover:scale-105 transform-gpu transition-all duration-500"
    >
      <Image
        class="w-10 aspect-square"
        src={"/favicon.png"}
        height={100}
        width={100}
        alt={BLOG_NAME}
      />

      <div class="ml-3 text-xl">
        {BLOG_NAME}
      </div>
    </a>
    <nav
      class="md:ml-auto hidden md:flex flex-row my-auto text-base justify-center"
    >
    </nav>
  </div>
</header>
